{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "matchables" :{
      ".indexOn" : ["elo_rating","match_type"],
      ".validate" : "newData.hasChildren(['owner'])"
    },
    "queue" : {
      "tasks": {
        ".read": "auth.admin === true",
        ".write":  "auth.canAddTasks === true || auth.admin === true",
        ".indexOn": "_state"
      }
    }
  }
}